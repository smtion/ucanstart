/******************** accordion ********************/

div.accordion {
    background-color: #fff;
    color: #444;
    cursor: pointer;
    padding: 18px;
    width: 100%;
    border-bottom:1px solid #ddd;
    text-align: left;
    outline: none;
    font-size:1.2em;
	font-weight:700;
    transition: 0.1s;
    background-image:url('../images/common/icon_arrow_down.png');
    background-position:97% 50%;
    background-repeat:no-repeat;
}
div.accordion span{
	width:95%;
	display:block;
}
div.accordion.active, div.accordion:hover {
    background-color: #fff;
	color:#d92742;
    
}
